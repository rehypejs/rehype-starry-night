{
  "name": "rehype-starry-night",
  "type": "module",
  "version": "1.0.2",
  "description": "syntaxhighlight using starry-night",
  "homepage": "https://github.com/y-nk/rehype-starry-night",
  "keywords": [
    "rehype",
    "plugin",
    "starry-night",
    "syntax",
    "hightlight"
  ],
  "author": "Julien Barbay <julien.barbay@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "files": [
    "./index.js"
  ],
  "dependencies": {
    "@wooorm/starry-night": "^3.0.0",
    "hast-util-select": "^6.0.0"
  },
  "devDependencies": {
    "prettier": "^3.0.0",
    "xo": "^0.58.0"
  },
  "scripts": {
    "format": "prettier . --log-level warn --write && xo --fix"
  },
  "prettier": {
    "bracketSpacing": false,
    "singleQuote": true,
    "semi": false,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false
  },
  "xo": {
    "prettier": true,
    "rules": {
      "logical-assignment-operators": "off"
    }
  }
}
